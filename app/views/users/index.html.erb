<div class="row">
  <div class="col-md-6 offset-md-3">
    <h1>ユーザー一覧</h1>

    <table class="table">
      <thead class="thead-dark">
      <tr>
        <th>名前</th>
      </tr>
      </thead>

      <tbody>
      <% @users.each do |user| %>
        <tr>
          <td><%= user.name %></td>
        </tr>
      <% end %>
      </tbody>
    </table>

    <br>

    <%= paginate @users %>
  </div>
</div>